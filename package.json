  "license": "ISC",
  "bugs": {
    "url": "https://github.com/douglasjam/blog/issues"
  },
  "homepage": "https://github.com/douglasjam/blog#readme",
  "devDependencies": {
    "autoprefixer": "^10.3.4",
    "concurrently": "^6.2.1",
    "cssnano": "^5.0.8",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.2.15"
  },
  "dependencies": {}
}
